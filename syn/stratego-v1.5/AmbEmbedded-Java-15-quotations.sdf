module AmbEmbedded-Java-15-quotations[M O]

exports 

  context-free syntax
    "|[" O "]|" -> M {cons("ToMetaExpr")}
    "#[" M "]"  -> O {cons("FromMetaExpr")}
  
  variables
    MetaVarID -> O

  lexical syntax 
    [A-Za-z\_\$][A-Za-z0-9\_\$]* -> MetaVarID
    JavaKeyword -> MetaVarID {reject}

  lexical restrictions
    MetaVarID -/- [A-Za-z0-9\_\$]

